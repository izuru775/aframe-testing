<a-scene>
  <a-assets>
    <img id="my-image" src="http://localhost:3001/images/mountain.jpg" />
  </a-assets>

  <!-- Using the asset management system. -->
  <a-image
    src="#my-image"
    position="0 1.5 -3"
    width="1"
    height="0.75"
    scale="3 3 1"
  ></a-image>
  <a-curvedimage
    src="#my-image"
    height="3.0"
    radius="5.7"
    rotation="0 100 0"
    theta-lenght="90"
    scale="0.8 0.8 0.8"
  ></a-curvedimage>
</a-scene>

